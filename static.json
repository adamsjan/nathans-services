{
    "root": "dist",
    "clean_urls": true,
    "routes": {
      "/**": "index.html"
    },
    "/**": {
      "Access-Control-Allow-Origin": "*",
      "X-Content-Type-Options": "nosniff"
    },
    "/**/*.js": {
      "Content-Type": "application/javascript"
      
    },
    "/**/*.css": {
      "Content-Type": "text/css"
    },
    "/**/*.png": {
      "Cache-Control": "max-age=31536000, immutable"
    },
    "/**/*.jpg": {
      "Cache-Control": "max-age=31536000, immutable"
    }
  }